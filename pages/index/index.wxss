/**index.wxss**/
.swiper swiper {
  width: 750rpx;
  height: 340rpx;
}
.swiper swiper image {
  width: 100%;
}
.navbar {
  display: flex;
}
.navbar .navbar-item {
  flex: 1;
  padding: 10px;
}
.navbar .navbar-item image {
  width: 100%;
}
.floor .floor-group .floor-title {
  margin: 10rpx;
  width: 100%;
}
.floor .floor-group .floor-list {
  overflow: hidden;
}
.floor .floor-group .floor-list navigator {
  float: left;
  width: 33.33%;
  height: 100%;
}
.floor .floor-group .floor-list navigator:nth-last-child(-n+4) {
  height: 27.72711207vw;
  border-left: 10rpx solid #fff;
}
.floor .floor-group .floor-list navigator:nth-child(2),
.floor .floor-group .floor-list navigator:nth-child(3) {
  border-bottom: 10rpx solid #fff;
}
.floor .floor-group .floor-list navigator image {
  width: 100%;
  height: 100%;
}
